USE GRAPH @graph

CREATE LOADING JOB load_job FOR GRAPH @graph {
	DEFINE FILENAME file_name;
	LOAD file_name
        TO VERTEX Person VALUES ($0, $0) WHERE $0 != "*Vertices" and $0 != "*Edges",
		TO EDGE Coworker VALUES ($0, $1, 1.0) WHERE $0 != "*Vertices" and $0 != "*Edges"
		USING Separator=" ", Header = "false";
}

